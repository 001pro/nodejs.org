<header>
    <div class="container">

        <img id="logo" src="/static/images/logo.svg" alt="node.js">
        <nav>
            <ul class="list-divider-pipe">
                <li{{#equals active "home"}} class="active"{{/equals}}>
                    <a href="{{site.home.link}}">{{site.home.text}}</a>
                </li>
                <li><a href="{{site.download.link}}">{{site.download.text}}</a></li>
                <li><a href="{{site.resources.link}}">{{site.resources.text}}</a></li>
                <li><a href="{{site.foundation.link}}">{{site.foundation.text}}</a></li>
                <li><a href="{{site.community.link}}">{{site.community.text}}</a></li>

                <li{{#equals active "about"}} class="active"{{/equals}}>
                  <a href="{{site.about.link}}">{{site.about.text}}</a>
                </li>
                <li{{#equals active "blog"}} class="active"{{/equals}}>
                  <a href="{{site.blog.link}}">{{site.blog.text}}</a>
                </li>
            </ul>
        </nav>

    </div>
</header>
